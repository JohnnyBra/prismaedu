{
  "extends": "eslint:recommended",
  "env": {
    "browser": true,
    "es2020": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "ecmaFeatures": {
        "jsx": true
    }
  },
  "rules": {
      "no-unused-vars": "warn",
      "no-undef": "error"
  }
}
